        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.activities.ParametroActivity" />
        <activity android:name=".modules.activities.GraficoActivity" />
        <activity android:name=".modules.activities.UsuarioListagemActivity" />
        <activity android:name=".modules.activities.UsuarioCadastrarEditarActivity" />
        <activity android:name=".modules.activities.HistoricoParametroActivity"></activity>
    </application>

</manifest>