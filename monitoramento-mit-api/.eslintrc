{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off"
    }
}